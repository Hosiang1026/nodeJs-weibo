<%- include header %>
<table class="table table-hover">
  <thead> 用户</thead>
  <tr>
  	<th>姓名</th>
  	<th>密码</th>
  	<th>邮箱</th>
  	<th>编辑</th>
  </tr>
  <% items.forEach(function(listitem){ %>
  	<tr>
  		<td><%= listitem.name %></td>
  		<td><%= listitem.password %></td>
  		<td><%= listitem.email %></td>
  		<td><button>编辑</button></td>
    </tr>
  <% }) %>
</table>
<table class="table table-hover">
  <thead>文章</thead>
  <tr>
  	<th>姓名</th>
  	<th>内容</th>
  	<th>编辑</th>
  </tr>
  <% posts.forEach(function(listitem){ %>
  	<tr>
  		<td><%= listitem.user %></td>
  		<td><%= listitem.post %></td>
  		
  		<td><button>编辑</button></td>
    </tr>
  <% }) %>
</table>

<%- include footer %>